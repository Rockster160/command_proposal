<div class="cmd-wrapper">
  <%= link_to "All Tasks", tasks_path %> | <%= link_to "Edit #{@task.session_type.humanize}", edit_task_path(@task) %>

  <h2><%= @task.name %></h2>
  <% if @task.description.present? %>
    <p><%= @task.description %></p>
  <% end %>

  <%= render partial: "#{@task.session_type}_show" %>
</div>
